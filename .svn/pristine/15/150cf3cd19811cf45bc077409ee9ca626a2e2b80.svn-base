//查询所有科室list
function showAllSection(){
	$.ajax({
		type : "POST",
		url : "<%=basePath%>showAllSection.php",
		data : {
			"evID":1
		},
		dataType : "json",
		async: true,
		success : function(data) {
			
			$.each(data,function(i,item){
				$("#section").append('<option>'+item.sectionName+'</option>');	
			});
			
		},
	});
}

