		var key = $("#key"+flagid).text();
		var val = $("#val"+flagid).val();
		
		//alert(flagid+"  "+key+"  "+val);
		 var s=key+" "+val;
		$("#elsediv"+flagid).remove();
		var mainPartys2="";
		var mainPartyssplit=mainPartys.split("/");
		if(mainPartyssplit.length!=1){
