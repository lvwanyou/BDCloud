.TTtabs {
  width: 650px;
  float: none;
  list-style: none;
  position: relative;
  margin: 0 0 0 0;
  text-align: left;
  padding: 0;
}
.TTtabs li {
  float: left;
  display: block;
  border-style: solid;
  border-width: 1px; 
  border-color: #4398D2;
  margin-top: 20px;
  color: #59A0D5;
  margin-right: 10px;
  width: 120px;
  text-align: center;
  line-height: 6px;
}
.TTtabs input[type="radio"] {
  position: absolute;
  top: -9999px;
  left: -9999px;
}
.TTtabs label {
  display: block;
  padding: 13px 21px;
  font-size: 12px;
  font-weight: normal;
  background: #E6F3F9;
  cursor: pointer;
  position: relative;
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
  font-family: "微软雅黑";
}
.TTtabs label:hover {

}
.TTtabs .TTtab-content {
  z-index: 2;
  display: none;
  overflow: hidden;
  width: 1200px;



  position: absolute;

  left: 0;
}
.TTtabs [id^="TTtab"]:checked + label {
	font-size: 12px;
  	background: #3992D0;
  	color: #FFFFFF;
  	font-family: "微软雅黑";
}
.TTtabs [id^="TTtab"]:checked ~ [id^="TTtab-content"] {
  display: block;
}



