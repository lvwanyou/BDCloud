				for(var i=1;i<=length;i++)
					  $("#order").remove();
				
				if(pagesum<sizes){
					var html2 = '<li class="active" id="order"><a href="#" onclick="SearchGHDB(1)">1</a></li >';
					$("#pages").after(html2);	
					
					
				}else{
				
				if(pageno<pagenum){
					if(pageno+length-1<=pagenum){
