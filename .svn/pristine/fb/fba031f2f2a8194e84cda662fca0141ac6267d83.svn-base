	<form class="form-inline" role="form" id="searchForm" action="<%=basePath%>/admin/logrizhi.php" method="post">
	<input type="hidden" name="pageno" id="pageno"/>
			<div class="row">
				<div class="panel panel-default">
					<div class="panel-heading"><span>搜索结果</span></div>
					<div class="panel-body" style="padding: 0 0 15px;">
						<table id="datatable" class="table table-striped table-hover"
							style="text-align: center; border-bottom: 1px solid #DDDDDD;">
							<thead>
								<tr>
									<th class="alcenter">操作</th>
									<th class="alcenter">用户名</th>
									<th class="alcenter">时间</th>
	
								</tr>
							</thead>
							<tbody id="tbcont">
							<c:forEach items="${logs }" var="cas">
								<tr data-toggle="modal" data-target="#myModal" onclick="searchone(this)">
									<td style="display: none;">${cas.id }</td>
									<td>${cas.content }</td>
									<td>${cas.username }</td>
									<td>${cas.logTime }</td>
									
							</c:forEach>
							</tbody>
						</table>
						<div class="alcenter" style="font-size: 14px">
							<div class="pagecount inline" style="height: 29px;">
