package com.xl.cloud.phoneBean;

//微信群相关页面对象
public class WeChatGroupAndMember {
	//群账号
	private String groupId;
	
	//群名称
	private String groupName;
	
	//群主号码
	private String groupLord;
	
	//群主昵称
	private String lordNick;
	
	//成员个数
	private String memberNum;
	
	//我在本群的昵称
	private String myNick;
	
	//成员昵称
	private String memberNick;
	
	//成员账号
	private String memberId;
	
	//成员个性签名
	private String memberSignature;
	
	//成员所在地
	private String memberLocation;
	
	//成员头像
	private String memberAvatar;
	
	//md5加密
	private String md5;
	
	public String getGroupId() {
		return groupId;
	}

	public void setGroupId(String groupId) {
		this.groupId = groupId;
	}

	public String getGroupName() {
		return groupName;
	}

	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}

	public String getGroupLord() {
		return groupLord;
	}

	public void setGroupLord(String groupLord) {
		this.groupLord = groupLord;
	}

	public String getLordNick() {
		return lordNick;
	}

	public void setLordNick(String lordNick) {
		this.lordNick = lordNick;
	}

	public String getMemberNum() {
		return memberNum;
	}

	public void setMemberNum(String memberNum) {
		this.memberNum = memberNum;
	}

	public String getMyNick() {
		return myNick;
	}

	public void setMyNick(String myNick) {
		this.myNick = myNick;
	}

	public String getMemberNick() {
		return memberNick;
	}

	public void setMemberNick(String memberNick) {
		this.memberNick = memberNick;
	}

	public String getMemberId() {
		return memberId;
	}

	public void setMemberId(String memberId) {
		this.memberId = memberId;
	}

	public String getMemberSignature() {
		return memberSignature;
	}

	public void setMemberSignature(String memberSignature) {
		this.memberSignature = memberSignature;
	}

	public String getMemberLocation() {
		return memberLocation;
	}

	public void setMemberLocation(String memberLocation) {
		this.memberLocation = memberLocation;
	}

	public String getMemberAvatar() {
		return memberAvatar;
	}

	public void setMemberAvatar(String memberAvatar) {
		this.memberAvatar = memberAvatar;
	}

	public String getMd5() {
		return md5;
	}

	public void setMd5(String md5) {
		this.md5 = md5;
	}

	@Override
	public String toString(){
		if(groupLord!=null){
			return "WeChatGroup [groupId=" + groupId 
					+ ", groupName=" + groupName
					+ ", groupLord=" + groupLord
					+ ", lordNick=" + lordNick
					+ ", memberNum=" + memberNum
					+ ", myNick=" + myNick
					+ ", md5=" + md5 + "]";
		}
		else{
			return "WeChatGroupMember [groupId=" + groupId 
					+ ", groupName=" + groupName
					+ ", memberNick=" + memberNick
					+ ", memberId=" + memberId
					+ ", memberSignature=" + memberSignature
					+ ", memberLocation=" + memberLocation
					+ ", memberAvatar=" + memberAvatar
					+ ", md5=" + md5 + "]";
		}
	}
	
}
