	//返译
		function no_trim(str) {
			
				if(str.indexOf("2P")){
					str=str.replace(/2P/g, "!");
				}
				if(str.indexOf("2O")){
					str=str.replace(/2O/g, "#");
				}
				if(str.indexOf("2I")){
					str=str.replace(/2I/g, "$");
				}
				if(str.indexOf("2U")){
					str=str.replace(/2U/g, "&");
				}
				if(str.indexOf("2Y")){
					str=str.replace(/2Y/g, "'");
				}
				if(str.indexOf("2E")){
					str=str.replace(/2E/g, "(");
				}
				if(str.indexOf("2Q")){
					str=str.replace(/2Q/g, ")");
				}
				if(str.indexOf("2A")){
					str=str.replace(/2A/g, "*");
				}
				
				if(str.indexOf("2W")){
					str=str.replace(/2W/g, " ");
				}
				if(str.indexOf("2B")){
					str=str.replace(/2B/g, "+");
				}
				if(str.indexOf("2C")){
					str=str.replace(/2C/g, ",");
				}
				if(str.indexOf("3A")){
					str=str.replace(/3A/g, ":");
				}
				if(str.indexOf("3B")){
					str=str.replace(/3B/g, ";");
				}
				
				if(str.indexOf("3D")){
					str=str.replace(/3D/g, "=");
				}
				if(str.indexOf("3F")){
					str=str.replace(/3F/g, "?");
				}
				if(str.indexOf("5B")){
					str=str.replace(/5B/g, "[");
				}
				
				if(str.indexOf("5D")){
					str=str.replace(/5D/g, "]");
				}
				if(str.indexOf(File.separator)){
					str=str.replace(File.separator,"\\" );
				}
				if(str.indexOf("2Z")){
					str=str.replace(/2Z/g, "%");
				}
				if(str.indexOf("7B")){
					str=str.replace(/7B/g, "{");
				}
				if(str.indexOf("7D")){
					str=str.replace(/7D/g, "}");
				}
				if(str.indexOf("7C")){
					str=str.replace(/7C/g, "|");
				}
				if(str.indexOf("7E")){
					str=str.replace(/7E/g, "~");
				}
				if(str.indexOf("6A")){
					str=str.replace(/6A/g, "`");
				}
				if(str.indexOf("5E")){
					str=str.replace(/5E/g, "^");
				}
				
				if(str.indexOf("5Q")){
					str=str.replace(/5Q/g, "-");
				}
				if(str.indexOf("5W")){
					str=str.replace(/5W/g, "ˬ");
				}
				if(str.indexOf("5R")){
					str=str.replace(/5R/g, "�");
				}
				return str;
		}