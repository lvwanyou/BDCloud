//开始导出
function exportDoc() {
	var checkboxs = document.getElementsByName("ids_url");
	var i = 0;
	var count = 0;
	var len = checkboxs.length;
	var urlstr = "";
	for(i = 0; i < len; i++){
		if(checkboxs[i].checked){	
			if(count == 0){
				urlstr += checkboxs[i].value;
			}else{
				urlstr += ","+checkboxs[i].value;
			}
			count = count + 1;
		}
	}
	if(urlstr==""){
		alert("请先选择导出文件");
	}
	else{
		 //对lookdocpath进行编码
		 var lookdocpath_encode=encodeURI(encodeURI(urlstr));
		 alert(lookdocpath_encode);
		 $("#url").attr("href","<%=basePath %>admin/exportDOC.php?docpath="+lookdocpath_encode); 
	}
		
}

