}

a {
	text-decoration: none;
}

.find_nav {
	background-color: #f9f9f9;
	position: relative;
	top: 0;
	z-index: 99;
	border-bottom: 1px solid #ddd;
	display: -moz-box;
	display: -webkit-box;
