	function getPicUrl(i) {
		var pUrl = document.getElementById("picUrl"+i).src;
		$.ajax({
			type: "POST",
			url: "<%=basePath %>admin/pictureGPS.php",
			dataType : "json",
			data : {
				"pUrl" : pUrl
			},						
			success : function(data) {
				$("#info_pic").empty();
				
				document.getElementById("container").style.display="";//显
				//地理位置展示
				showPicGPS(data.longitude,data.latitude);
				
				var str="拍摄机型："+data.shootTool+"&nbsp;&nbsp;&nbsp;&nbsp;拍摄日期："+data.date_photograph;
				$("#info_pic").append(str);
			},						
			error : function(data) {
			}
		});
	}
